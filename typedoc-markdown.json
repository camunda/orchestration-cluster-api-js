{
  "entryPoints": ["src/index.ts"],
  "out": "docs-md",
  "plugin": ["typedoc-plugin-markdown", "typedoc-docusaurus-theme", "typedoc-plugin-frontmatter"],
  "groupOrder": ["Classes", "Functions", "Interfaces", "Namespaces", "Type Aliases"],
  "exclude": [
    "**/node_modules/**",
    "**/tests/**",
    "**/tests-integration/**",
    "camundacon/**",
    "scripts/**"
  ],
  "excludePrivate": true,
  "excludeProtected": true,
  "hideGenerator": true,
  "skipErrorChecking": true,
  "frontmatterGlobals": {
    "mdx" : {
      "format": "md"
    }
  },
  "sidebar": {
    "pretty": true
  }, 
  "docsPath": "apis-tools/typescript/oca-client-docs"
}
